# Operations on graph
## Subgraph
h <- induced.subgraph(g, 1:5)
str(h)
h1 <- g - vertices(c(6,7))
h1
h2 <- h1 + vertices(c(6,7))
h2
g <- h + edges(c(4,6),c(4,7),c(5,6),c(6,7))
g <- h2 + edges(c(4,6),c(4,7),c(5,6),c(6,7))
g
plot(g)
## Union
h3 <- h
h4 <- graph.formula(4-6, 4-7, 5-6, 6-7)
g1 <- graph.union(h3,h4)
## Disjoint Union
g2<- h3 %du% h4
plot(g2)
g3<- g %s% h
plot(g3)
g4<- g %m% h
plot(g4)
g5<-g.complementer()
plot(g5)
g5<-complementer(g)
plot(g5)
